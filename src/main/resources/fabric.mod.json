{
  "schemaVersion": 1,
  "id": "bluemap-banners",
  "version": "${version}",
  "name": "BlueMap Banners",
  "description": "A Minecraft Fabric mod that adds banners to BlueMap.",
  "authors": ["RealMuffinTime"],
  "contributors": ["Nincodedo", "ekul6547"],
  "contact": {
    "homepage": "https://modrinth.com/mod/bluemap-banners",
    "repo": "https://github.com/RealMuffinTime/bluemap-banners",
    "issues": "https://github.com/RealMuffinTime/bluemap-banners/issues"
  },
  "license": "MIT",
  "icon": "assets/bluemap-banners/icon.png",
  "environment": "server",
  "entrypoints": {
    "main": [
      "dev.nincodedo.bluemapbanners.BlueMapBanners"
    ]
  },
  "mixins": [
    "bluemap-banners.mixins.json"
  ],
  "depends": {
    "bluemap": ">=${bluemap_version}",
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": ">=${minecraft_version}"
  }
}
